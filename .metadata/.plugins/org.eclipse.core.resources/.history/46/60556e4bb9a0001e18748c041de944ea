package com.mathodReferance;

import java.util.function.Function;

public class MethodReferanceDemo {

	public static void main(String[] args) {


		Function<String, String> lambdaFunction=(String s)->s.toUpperCase();
		
		System.out.println(lambdaFunction.apply("alakuntlavasu"));
		
		
		Function<String, String> methodRefernce= String:: toLowerCase;
		
		System.out.println(methodRefernce.apply("ALAKUNTLAVASU"));
		
		

	}

}
